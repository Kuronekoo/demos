<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.sc.SSHDemo.beans.DepartmentEntity" table="department" schema="demo">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="departmentName">
            <column name="department_name" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>