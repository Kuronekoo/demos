<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="dubbo_server"  />
 
    <!-- 使用multicast广播注册中心暴露服务地址 -->
    <!--<dubbo:registry address="zookeeper://127.0.0.1:2181" />-->
    <dubbo:registry address="zookeeper://10.238.6.147:2181" />
    <!-- 用dubbo协议在20880端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="5734" />
 
    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service ref="UserServiceImpl" interface="com.yunji.oa.service.IUserService" timeout="60000000"/>
</beans>